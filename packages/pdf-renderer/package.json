{
  "name": "pdf-renderer",
  "version": "1.0.0",
  "type": "module",
  "description": "PDF renderer using Puppeteer to convert scpdf HTML to PDF",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist"
  },
  "dependencies": {
    "scpdf": "workspace:*",
    "puppeteer": "^23.0.0",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
